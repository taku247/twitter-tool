{
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "numReplicas": 1,
    "sleepApplication": false,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 3
  },
  "runtime": {
    "memory": "512MB",
    "cpu": "0.5"
  },
  "networking": {
    "serviceDomain": "twitter-worker"
  }
}